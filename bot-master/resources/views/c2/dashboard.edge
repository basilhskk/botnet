<!DOCTYPE html>
<html>
<head>
 @include("inc/head")
</head>
<body class="hold-transition sidebar-mini layout-fixed">
<div class="wrapper">

  <!-- Navbar -->
  @include("inc/topnav")
  <!-- /.navbar -->

  <!-- Main Sidebar Container -->
  @include("inc/sidebar")

  <!-- Content Wrapper. Contains page content -->
  <div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <div class="content-header">
      <div class="container-fluid">
        <div class="row mb-2">
          <div class="col-sm-6">
            <h1 class="m-0 text-dark">C2 Servers</h1>
          </div><!-- /.col -->
          <div class="col-sm-6">
            <ol class="breadcrumb float-sm-right">
              <li class="breadcrumb-item"><a href="#">c2</a></li>
              <li class="breadcrumb-item active">Dashboard</li>
            </ol>
          </div><!-- /.col -->
        </div><!-- /.row -->
      </div><!-- /.container-fluid -->
    </div>
    <!-- /.content-header -->

    <!-- Main content -->
    <section class="content">
      <div class="container-fluid">

        

        <!-- Small boxes (Stat box) -->
         @each(row in rows)
        <div class="row">
            @each(server in row.servers)
            <div class="col-lg-3 col-6">
                <!-- small box -->
                @if(server.os == "win32")
                <div class="small-box bg-success">
                @else
                <div class="small-box bg-info">
                @endif
                <div class="inner">
                    <h4>CnC</h4>
                    <h4>OS: {{server.os}}</h3>
                </div>
                <div class="icon">
                    <i class="fas fa-server"></i>
                </div>
                <a href="/c2/server/{{server.id}}" class="small-box-footer">More info <i class="fas fa-arrow-circle-right"></i></a>
                </div>
            </div>
            @endeach
        </div>
        @endeach

        
      </div><!-- /.container-fluid -->
    </section>
    <!-- /.content -->
  </div>
  <!-- /.content-wrapper -->
  @include("inc/footer")


  
</div>
<!-- ./wrapper -->

@include("inc/scripts")
</body>
</html>
